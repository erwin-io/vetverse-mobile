{
  "appId": "io.ionic.starter",
  "appName": "Vetverse",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "server": {
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 10000,
      "launchAutoHide": false
    },
    "PushNotifications": {
      "sound": "notif_alert",
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    },
    "LocalNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ],
      "smallIcon": "ic_notification_round",
      "largeIcon": "ic_notification_round"
    }
  },
  "cordova": {}
}
